#include <stdio.h>

int main() {
   int arr[] = {12, 56, 34, 78, 100};
      int n = sizeof(arr)/sizeof(arr[0]); // get the size of the array
         int max = arr[0]; // initialize max to the first element in the array
   for (int i = 1; i < n; i++) {
         if (arr[i] > max) {
                  max = arr[i];
                        }
                           }
   printf("The largest element in the array is: %d", max);
      return 0;
      }
